{
  "name": "noche",
  "description": "An apache-style-configuration web server written in node.",
  "keywords": [
    "web",
    "server",
    "webserver",
    "apache",
    "directoryindex",
    "html",
    "json"
  ],
  "version": "0.0.1",
  "author": "Blake Stephens <brak.stephens@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Djspaceg/noche.git"
  },
  "main": "server.ts",
  "directories": {
    "conf": "./conf"
  },
  "scripts": {
    "start": "ts-node server.ts",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm-check-updates": "npx npm-check-updates",
    "npm-do-minor-updates": "npm run npm-check-updates -- -u --target=minor",
    "npm-do-major-updates": "npm run npm-check-updates -- -u --target=latest",
    "npm-do-patch-updates": "npm run npm-check-updates -- -u --target=patch"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "less-middleware": "^3.1.0",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/ejs": "^3.1.2",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.17",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.3",
    "@types/serve-favicon": "^2.5.4",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
